{
    "manifest_version": 3,
    "name": "Efex Password Gen Extension",
    "version": "1.0",
    "permissions": ["contextMenus", "activeTab"],
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
        },
        {
          "matches": ["https://burst.transmitsms.com/*"],
          "js": ["burstcontent.js"]
        }
      ],
    "background": {
      "service_worker": "background.js"
    }
  }
  